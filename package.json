{
  "name": "azora-os-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "apps/*"
  ],
  "scripts": {
    "start": "docker compose up -d --build",
    "stop": "docker compose down",
    "dev": "concurrently \"npm run dev --workspace=apps/staff-pwa\" \"npm run dev --workspace=apps/security-dashboard\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
